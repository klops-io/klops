{
    "name": "klops the octopus",
    "version": "1.0.0",
    "description": "carbon analysis of your web searches",
    "manifest_version": 3,
    "author": "klop gang",
    "browser_action": {
        "browser_style": true,
        "default_popup": "index.html",
        "default_title": "Klops the Octopus"
      },
    "permissions": [
        "storage",
        "webRequest",
        "<all_urls>"
    ]
}